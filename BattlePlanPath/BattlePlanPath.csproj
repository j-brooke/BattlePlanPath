<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>

    <PackageId>BattlePlanPath</PackageId>
    <Version>2.0.0</Version>
    <Authors>Jesse Brooke</Authors>
    <Copyright>Copyright 2020 Jesse Brooke</Copyright>
    <PackageDescription>A simple but flexible implementation of the A* (A-Star) pathfinding algorithm for  .NET Standard.</PackageDescription>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageTags>pathfinding; A*; AStar</PackageTags>
    <RepositoryUrl>https://github.com/j-brooke/BattlePlanPath.git</RepositoryUrl>
    <PackageProjectUrl>https://github.com/j-brooke/BattlePlanPath</PackageProjectUrl>
  </PropertyGroup>

</Project>
